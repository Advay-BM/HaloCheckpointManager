<Window x:Class="WpfApp3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title="Halo Checkpoint Manager 0.9.2b" Height="593" Width="820"
        MinHeight="593" MinWidth="555" Loaded="Window_Loaded">


    <Window.Resources>
        <Style TargetType="ListViewItem">
            <Setter Property="HorizontalContentAlignment" Value="Stretch" />
            <Setter Property="Padding" Value="0" />
            <Setter Property="MinWidth" Value="0" />
            <Setter Property="MinHeight" Value="0" />
        </Style>



    </Window.Resources>

    <Grid>

        <Grid.Resources>
            <Style TargetType="ListViewItem">
                <Style.Triggers>
                    <Trigger Property="ItemsControl.AlternationIndex"  Value="0">
                        <Setter Property="Background" Value="White" />
                    </Trigger>
                    <Trigger Property="ItemsControl.AlternationIndex"  Value="1">
                        <Setter Property="Background" Value="WhiteSmoke" />
                    </Trigger>
                </Style.Triggers>
            </Style>
        </Grid.Resources>

        <TabControl  x:Name="TabList" Margin="0,30,0,0" SelectionChanged="TabSelectionChanged">


            <TabItem Header="H1 Cores">
                <Grid x:Name="H1CS" Background="#FFE5E5E5" Margin="0,0,0,-4">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="13*"/>
                        <ColumnDefinition Width="55*"/>
                        <ColumnDefinition Width="738*"/>
                    </Grid.ColumnDefinitions>
                    <ListView Margin="10,10,335,20" x:Name="CS_MainList" SelectionMode="Single" AlternationCount="2" Grid.ColumnSpan="3" ItemsSource="{Binding Halo1CoreSaves}">
                        <ListView.View>
                            <GridView>

                                <GridViewColumn Header="Lvl" Width="36" DisplayMemberBinding="{Binding LevelCode}"/>
                                <GridViewColumn Header="Diff" Width="28">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <Image  Source="{Binding DifficultyImageH1}" Width="17" Height="18"/>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Time" Width="40" DisplayMemberBinding="{Binding TimeString}"/>
                                <GridViewColumn Header="Name" Width="auto" DisplayMemberBinding="{Binding Name}">
                                    <GridViewColumn.HeaderContainerStyle>
                                        <Style TargetType="{x:Type GridViewColumnHeader}">
                                            <Setter Property="HorizontalContentAlignment" Value="Left" />
                                        </Style>
                                    </GridViewColumn.HeaderContainerStyle>
                                </GridViewColumn>

                            </GridView>
                        </ListView.View>
                    </ListView>
                    <TextBox x:Name="H1CS_Loa_LevelName" HorizontalAlignment="Right" Height="21" Margin="0,128,44,0" TextWrapping="Wrap" Text="N/A" VerticalAlignment="Top" Width="121" IsEnabled="False" Grid.Column="2"/>
                    <TextBox x:Name="H1CS_Loa_Time" HorizontalAlignment="Right" Height="21" Margin="0,92,44,0" TextWrapping="Wrap" Text="N/A" VerticalAlignment="Top" Width="81" IsEnabled="False" Grid.Column="2"/>
                    <TextBox x:Name="H1CS_Sel_FileName" HorizontalAlignment="Right" Height="21" Margin="0,240,44,0" TextWrapping="Wrap" Text="N/A" VerticalAlignment="Top" Width="261" IsEnabled="False" Grid.Column="2"/>
                    <TextBox x:Name="H1CS_Sel_LevelName" HorizontalAlignment="Right" Height="21" Margin="0,308,44,0" TextWrapping="Wrap" Text="N/A" VerticalAlignment="Top" Width="121" IsEnabled="False" Grid.Column="2"/>

                    <TextBox x:Name="H1CS_Sel_Time" HorizontalAlignment="Right" Height="21" Margin="0,272,44,0" TextWrapping="Wrap" Text="N/A" VerticalAlignment="Top" Width="81" IsEnabled="False" Grid.Column="2"/>
                    <Button x:Name="H1CS_Loa_DumpButton" Content="Dump" HorizontalAlignment="Right" Height="26" Margin="0,174,223,0" VerticalAlignment="Top" Width="82" Click="DumpButton_Click" Grid.Column="2"/>
                    <Image x:Name="H1CS_Loa_LevelImage" HorizontalAlignment="Right" Height="68" Margin="0,92,184,0" VerticalAlignment="Top" Width="121"  Grid.Column="2"/>
                    <Image x:Name="H1CS_Sel_LevelImage" HorizontalAlignment="Right" Height="68" Margin="0,272,184,0" VerticalAlignment="Top" Width="121"  Grid.Column="2"/>
                    <Button x:Name="H1CS_Sel_InjectButton" Content="Inject" HorizontalAlignment="Right" Height="26" Margin="0,354,223,0" VerticalAlignment="Top" Width="82" Click="InjectButton_Click" Grid.Column="2" />
                    <Button x:Name="H1CS_Sel_RenameButton" Content="Rename" HorizontalAlignment="Right" Height="26" Margin="0,404,184,0" VerticalAlignment="Top" Width="121" Click="RenameButton_Click" Grid.Column="2"/>
                    <Button x:Name="H1CS_Sel_InjectRevertButton" Content="Inject &amp; Core Load" HorizontalAlignment="Right" Height="26" Margin="0,354,44,0" VerticalAlignment="Top" Width="157" Click="InjectRevertButton_Click" Grid.Column="2"/>
                    <Button x:Name="H1CS_Sel_DeleteButton" Content="Delete" HorizontalAlignment="Right" Height="26" Margin="0,404,44,0" VerticalAlignment="Top" Width="121" Click="DeleteButton_Click" Grid.Column="2"/>
